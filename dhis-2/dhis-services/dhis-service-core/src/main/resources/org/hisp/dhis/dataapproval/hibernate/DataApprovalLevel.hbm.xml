<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"
  [<!ENTITY identifiableProperties SYSTEM "classpath://org/hisp/dhis/common/identifiableProperties.hbm">
  <!ENTITY tablename "dataapprovallevel">]
  >
<hibernate-mapping>
  <class name="org.hisp.dhis.dataapproval.DataApprovalLevel" table="&tablename;">

    <cache usage="read-write" />

    <id name="id" column="dataapprovallevelid">
      <generator class="native" />
    </id>
    &identifiableProperties;

    <property name="name" column="name" not-null="true" unique="true" length="230" />

    <property name="translations" type="jblTranslations"/>

    <property name="level" column="level" not-null="true" index="in_dataapprovallevel_level" />

    <properties name="uk_dataapprovallevel_orgunitlevel_categoryoptiongroupset_unique_key" unique="true">
      <property name="orgUnitLevel" column="orgunitlevel" not-null="true" />
      <many-to-one name="categoryOptionGroupSet" class="org.hisp.dhis.category.CategoryOptionGroupSet"
        column="categoryoptiongroupsetid" foreign-key="fK_dataapprovallevel_categoryoptiongroupsetid" not-null="false" />
    </properties>

    <!-- Access properties -->
    <many-to-one name="user" class="org.hisp.dhis.user.User" column="userid" foreign-key="fk_dataapprovallevel_userid" />

    <property name="publicAccess" length="8" />

  <set name="userGroupAccesses" table="dataapprovallevelusergroupaccesses" cascade="all-delete-orphan">
      <cache usage="read-write" />
      <key column="dataapprovallevelid" foreign-key="fk_dataapprovallevel_usergroupaccessid" />
      <many-to-many class="org.hisp.dhis.user.UserGroupAccess" foreign-key="fk_usergroupaccess_dataapprovallevel">
            <column name="usergroupaccessid" unique-key="uk_dataapprovallevel_usergroupaccessid"/>
 </many-to-many>
    </set>

  <set name="userAccesses" table="dataapprovalleveluseraccesses" cascade="all-delete-orphan">
      <cache usage="read-write" />
      <key column="dataapprovallevelid" foreign-key="fk_dataapprovallevel_useraccessid" />
      <many-to-many class="org.hisp.dhis.user.UserAccess" foreign-key="fk_useraccess_dataapprovallevel">
            <column name="useraccessid" unique-key="uk_dataapprovallevel_useraccessid"/>
 </many-to-many>
    </set>

  </class>

</hibernate-mapping>
